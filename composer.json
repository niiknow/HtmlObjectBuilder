{
  "name": "niiknow/ObjectHtmlBuilder",
  "description": "to build HTML from PHP object, array, or json string",
  "keywords": [
    "html",
    "formbuilder",
    "form-builder"
  ],
  "license": "MIT",
  "authors": [{
    "name": "Tom Noogen",
    "email": "friends@niiknow.org"
  }],
  "type": "library",
  "require": {
    "php": ">=7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.3.0"
  },
  "autoload": {
    "psr-4": {
      "niiknow\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "prefer-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
